<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Doctor Name</th>
      <th scope="col">Specialization</th>
      <th scope="col">User Email</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @doctors.each do |doctor|%>
       <tr>
        <th scope="row"><%= doctor.id%></th>
        <td><%= doctor.name%></td>
        <td><%= doctor.specialization%></td>
        <td><%= doctor.user.email%></td>
        <td><%= link_to "edit", edit_doctor_path(doctor.id)%></td>
        <td><%= link_to "show", doctor_path(doctor.id)%></td>
        <td><%= button_to "delete", doctor_path(doctor.id), method: :delete %></td>
      </tr>
    <%end%>
  </tbody>
</table>
